/*
 * Exemplo de uso:
 * Sensor de Cor
 */

#define COLORSENSOR SENSOR_2

task main() {

    while(true) {
        SetSensorColorFull(IN_2);

        Wait(300);

        ClearScreen();

		//COLORSENSOR retornar√° 1, 2, 3, 4, 5 ou 6
 		//do mais escuro(1) para o mais claro(6).
        NumOut(0,0, COLORSENSOR);
    }
}