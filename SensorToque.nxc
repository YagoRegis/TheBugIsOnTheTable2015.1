/*
 * Exemplo de uso:
 * Sensor de Toque
 */

task main() {

	// Indica a porta que o sensor est√° conectado, neste caso porta 1.
    SetSensorTouch(IN_1);

    while(true){
        if(Sensor(IN_1) == 1) { 
            OnFwd(OUT_AC, 100);  
        }
        else {
            Off(OUT_AC);
        }
    }
} 